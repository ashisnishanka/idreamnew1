pipeline{

agent any
stages{

stage('Build')
{
 steps{
 echo "Build the code...."
 sh "export PATH=$MVN_CMD_DIR:$PATH:opt/bin && mvn clean test"
 
 }
 }
stage('Test')
{
 steps{
 echo "Testing the code...."
 sh "mvn test"
 
 } 
 }
 
 stage('Compile')
{
 steps{
 echo "Testing the code...."
 sh "mvn compile"
 
 } 
 
 }

}

}
